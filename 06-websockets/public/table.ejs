<% if (products.length !== 0) { %>
    <table>
        <tr>
            <th>Nombre</th>
            <th>Precio</th>
            <th>Imagen</th>
        </tr>
        <% products.forEach(product => { %>
            <tr>
                <td><%= product.title %> </td>
                <td><%= product.price %> </td>
                <td><img src=<%= product.image %> alt="Imagen"> </td>
            </tr>
        <% }); %>
    </table>
<% } else { %>
    <h3>No hay productos para mostrar</h3>
<% } %>